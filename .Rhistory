varcov1 <- matrix(c(10, 2, 2, 10), ncol = 2, nrow = 2)
varcov2 <- matrix(c(10, 5, 5, 10), ncol = 2, nrow = 2)
varcov3 <- matrix(c(10, 7, 7, 10), nrow = 2, ncol = 2)
